{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    <div>
        {% if object.energyLevel == 1 %}
            <strong>Chillout</strong>
        {% elseif object.energyLevel == 2 %}
            <strong>Casual</strong>
        {% elseif object.energyLevel == 3 %}
            <strong>Party</strong>
        {% endif %}
    </div>
{% endblock %}