<div class="margin-bottom-30">
    <div class="block">
        <div class="side-ad margin-bottom-20" data-ad-tag="{{ ad.tag }}">
            {% if ad.link is not empty %}<a onclick="_gaq.push(['_trackEvent', 'Ads', 'Ads click', '{{ ad.tag }}']);" href="{{ ad.link }}" target="_blank" rel="nofollow">{% endif %}
                <img src="{{ showImage(ad.image, 'ads_300_600') }}" alt="{{ ad.name }}" width="300" height="600">
            {% if ad.link is not empty %}</a>{% endif %}
            <div class="txt" style="bottom: 2%">Advertising</div>
        </div>
    </div>
</div>
