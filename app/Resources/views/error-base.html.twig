<html>
<head>
    <meta charset="UTF-8">
    <title>404</title>

    <!-- CSS -->
    <link rel="stylesheet" type="text/css" href="stylesheets/front.css">
    <link rel="stylesheet" type="text/css" href="stylesheets/main.css">

    <!-- JS -->
    <script type="text/javascript" src="js/plugins/jquery.min.js"></script>
    <script type="text/javascript" src="js/plugins/jquery.easing.js"></script>
    <script type="text/javascript" src="js/plugins/modernizr.js"></script>
    <script type="text/javascript" src="js/libs/utils.metabolism.js"></script>
    <script type="text/javascript" src="js/app.js"></script>
</head>
<body>
<header class="desktop">
    <div class="container">
        <div class="twelve columns">
            <table>
                <tbody>
                    <tr>
                        <td class="logo">
                            <a href="{{ url('homepage') }}">
                                <img src="{{ asset('bundles/wbbcore/images/logo-placeholder.png') }}" alt="logo" width="157" height="85"/>
                            </a>
                        </td>
                        <td class="nav">
                            <nav class="h3">
                                <a href="{{ path('wbb_cities') }}">Cities</a>
                                <a href="{{ path('wbb_bar_guides') }}" >Bar Guides</a>
                                <a href="{{ path('wbb_news_landing_page') }}" >News</a>
                            </nav>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
</header>

<div class="full-cream">
    <div id="error" class="min-height">
        <div class="padding-top-80 wrap">
            <div id="alert" class="text-align-center text-transform-uppercase">
                <img src="images/404_desktop.png">
                <div class="subtitle margin-top-30">404 ERROR</div>
                <p class="margin-top-20 padding-bottom-20">The page you are looking for <br />
                    has either been moved <br />or removed</p>
            </div> <!-- #success -->
        </div> <!-- #UI -->
    </div> <!-- #register -->
</div>

{% block footer %}
    {% include '::footer.html.twig' %}
{% endblock %}

</body>
</html>